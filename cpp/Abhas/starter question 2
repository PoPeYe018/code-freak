#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,m;
        cin>>n>>m;
        int x,y;
        cin>>x>>y;
        while(n--)
        {
            int F=0,P=0,U=0;
            for(int i=0;i<m;i++)
            {
                char ch;
                cin>>ch;
                if(ch =='F')
                F++;
                else if(ch =='U')
                U++;
                else 
                P++;
            }
            if(F>=x)
            cout<<"1";
            else if(F==(x-1) and P>=y)
            cout<<"1";
            else cout<<"0";
        }
        cout<<endl;
    }
    return 0;
}
