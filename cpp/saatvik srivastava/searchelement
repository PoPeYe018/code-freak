#include<iostream>
using namespace std;

int linearSearch(int arr[] ,int n,int key){
    for(int i=0;i<n;i++){
        if(arr[i]==key){
            return cout<<"element fount at ",i;
        }
    }
    return -1;
}

int main(){
    int n ;
    cout<<"Enter value of n\n";
    cin>>n;
    int arr[n];
    cout<<"Enter elemet of array\n";
    for(int i=0;i<n;i++){

        cin>>arr[i];

    }
    int key;
    cout<<"Enter key to be searched\n";
    cin>>key;
    cout<<linearSearch(arr , n, key)<<endl;
    return 0;
}
